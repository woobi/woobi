{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["woobi.js","./lib","README.md"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": ""
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "encoding": "utf8",               
        "destination": "./docs/",         
        "recurse": true                
    }
}

